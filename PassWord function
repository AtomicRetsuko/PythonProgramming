def askPassWord(mdp):
    passWord = (mdp)
    asw = ""
    tentative = 0
    while (asw != mdp):
        asw = input("Password ?")
        if asw != passWord:
            print("Wrong password, please try again")
            tentative += 1
    print(f"Welcome Home Pingu, You got your PassWord good in {tentative} atempts")
